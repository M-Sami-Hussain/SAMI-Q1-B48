//==========================Operations that result in number 8 =============================
console.log ('All operations that ressult in  the number eight(8)');
console.log("Addition");
console.log(5+3);
console.log("Subtraction");
console.log(10-2);
console.log("Multiplication");
console.log(4*2);
console.log("Division");
console.log(16/2);
